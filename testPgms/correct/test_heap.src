program test_program is
    global variable tmp : integer;
    procedure print_string : integer(variable level : integer)
        variable s : string;
        variable x : bool;
    begin
        x := putString("Enter a string:");
        s := getString();
        if (level < 3) then
            tmp := print_string(level + 1);
        end if;
        x := putString(s);
	//return s;
	return tmp;
    end procedure;
begin
    tmp := print_string(2);
end program.
